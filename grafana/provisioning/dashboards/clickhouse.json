{
  "dashboard": {
    "title": "ClickHouse Ozon Analytics",
    "tags": ["clickhouse", "ozon", "analytics"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Database Queries Per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ClickHouseProfileEvent_Query[5m])",
            "legendFormat": "Queries"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "ClickHouseProfileEvent_MemoryTracking",
            "legendFormat": "Memory Used"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "title": "Active Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "ClickHouseProfileEvent_TCPConnection",
            "legendFormat": "Connections"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      }
    ]
  }
}